---
import Greetings from '../components/Greetings';
import AnsiText from '../components/Header';
import Navbar from '../components/Navbar';
import FancyTerminal from '../components/Terminal';
import Terminal from '../layouts/terminal.astro';
---

<Terminal title="Home - EuzDev">
	<Navbar client:load/>
	<div class="mt-3">
		<AnsiText message='home'/>
	</div>
	<div class="flex flex-col flex-1 px-1 md:px-12">
		<Greetings client:visible/>
		<FancyTerminal client:only/>
	</div>
</Terminal>